<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">

        <shape android:shape="rectangle">

            <!--    테두리-->
            <stroke
                android:width="1dp"
                android:color="#FFFFFF" />

            <!--    배경 색상-->
            <solid
                android:color="@android:color/transparent" />

        </shape>

    </item>

</layer-list>